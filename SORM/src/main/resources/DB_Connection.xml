<?xml version="1.0" encoding="UTF-8" ?>
<connection xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DB_Connection.xsd">

<!--    Edit the below element bodies to define database connection information-->
<!--    database location-->
    <url>jdbc:h2:mem:tcp://localhost/~/test;MODE=PostgreSQL;</url>

<!--    current schema, may optionally be left empty to use default schema-->
    <schema></schema>

<!--    user to be logged in as-->
    <user>sa</user>

<!--    user's password, may optionally be left empty if user requires no password-->
    <password></password>
</connection>
